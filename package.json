{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "react boiler plate",
  "main": "index.js",
  "engines": {
    "node": "12.18.2",
    "npm": "6.14.5"
  },
  "scripts": {
    "backend": "nodemon server/index.js",
    "frontend": "npm run start --prefix client",
    "start": "node server/index.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "John ahn",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.737.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "heroku": "^7.42.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "react-dropzone": "^11.0.2",
    "react-redux": "^5.0.7",
    "saslprep": "^1.0.3",
    "should-send-same-site-none": "^2.0.5",
    "supports-color": "^7.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
